\begin_layout Paragraph*
1er GDL
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A_{0}^{1}\downarrow=\left[\begin{array}{cccc}
c_{q1} & 0 & S_{q1} & L_{1}c_{q1}\\
s_{q1} & 0 & -c_{q1} & L_{1}s_{q1}\\
0 & 1 & 0 & 0\\
0 & 0 & 0 & 1
\end{array}\right]\label{eq:dh1}
\end{equation}

\end_inset


\end_layout

\begin_layout Paragraph*
2do GDL
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A_{1}^{2}\downarrow=\left[\begin{array}{cccc}
c_{q2} & -s_{q2} & 0 & L_{2}c_{q2}\\
s_{q2} & c_{q2} & 0 & L_{2}s_{q2}\\
0 & 0 & 1 & 0\\
0 & 0 & 0 & 1
\end{array}\right]\label{eq:dh2}
\end{equation}

\end_inset


\end_layout

\begin_layout Paragraph*
3er GDL
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A_{2}^{3}\downarrow=\left[\begin{array}{cccc}
c_{q3} & -s_{q3} & 0 & L_{3}c_{q3}\\
s_{q3} & c_{q3} & 0 & L_{3}s_{q3}\\
0 & 0 & 1 & 0\\
0 & 0 & 0 & 1
\end{array}\right]\label{eq:dh3}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Para obtener la cinemática directa total se multiplican las matrices de
 manera que 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $A_{0}^{3}\downarrow=A_{0}^{1}\downarrow\times A_{1}^{2}\downarrow\times A_{2}^{3}\downarrow$
\end_inset

.
\end_layout

\begin_layout Standard
Finalmente se obtiene:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A_{0}^{3}\downarrow=\left[\begin{array}{cccc}
c_{q1}c_{q2+q3} & -c_{q1}s_{q2+q3} & s_{q1} & c_{q1}\bigl(L_{3}(c_{q2}c_{q3}-s_{q2}s_{q3})+L_{2}c_{q2}+L1\bigr)\\
s_{q1}c_{q2+q3} & -s_{q1}s_{q2+q3} & -c_{q1} & s_{q1}\bigl(L_{3}(c_{q2}c_{q3}-s_{q2}s_{q3})+L_{2}c_{q2}+L1\bigr)\\
s_{q2+q3} & c_{q2+q3} & 0 & L_{3}(s_{q2}c_{q3}+c_{q2}s_{q3})+L_{2}s_{q2}\\
0 & 0 & 0 & 1
\end{array}\right]\label{eq:dhf}
\end{equation}

\end_inset


\end_layout

